# Productboard API Configuration
PRODUCTBOARD_API_TOKEN=
PRODUCTBOARD_API_URL=https://api.productboard.com

# Owner Email Aliases (for privacy-preserving filtering)
# Add aliases for team members to filter notes/features by owner without exposing emails
# Usage: node productboard.js all-notes --owner alice
# OWNER_EMAIL_ALICE=alice@example.com
# OWNER_EMAIL_BOB=bob@example.com

# Dovetail API Configuration
DOVETAIL_API_TOKEN=your_dovetail_api_token_here
DOVETAIL_API_URL=https://dovetail.com/api/v1/

# Atlassian API Configuration (Optional)
ATLASSIAN_API_TOKEN=
ATLASSIAN_SITE_URL=https://ardoqcom.atlassian.net

# PII Filtering Configuration
PII_FILTER_ENABLED=true
PII_ANONYMIZE_EMAILS=true
PII_ANONYMIZE_NAMES=true
PII_ANONYMIZE_PHONE=true
